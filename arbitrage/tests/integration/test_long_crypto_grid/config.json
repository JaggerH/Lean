{
  "comment": "Long Crypto Grid Strategy Test - Grid Trading Framework Integration",
  "comment2": "IBKR Account: $50,000 with 2x leverage | Kraken Account: $50,000 with 5x leverage",

  "environment": "backtesting",
  "algorithm-type-name": "LongCryptoGridTest",
  "algorithm-language": "Python",
