{
  "environment": "live-paper",
  "live-mode": true,
  "data-queue-handler": "QuantConnect.Lean.Engine.DataFeeds.LiveTradingDataFeed",
  "live-mode-brokerage": "PaperBrokerage",
  "setup-handler": "QuantConnect.Lean.Engine.Setup.BrokerageSetupHandler",
  "result-handler": "QuantConnect.Lean.Engine.Results.LiveTradingResultHandler",
  "data-feed-handler": "QuantConnect.Lean.Engine.DataFeeds.LiveTradingDataFeed",
  "real-time-handler": "QuantConnect.Lean.Engine.RealTime.LiveTradingRealTimeHandler",
  "transaction-handler": "QuantConnect.Lean.Engine.TransactionHandlers.BrokerageTransactionHandler",
  "history-provider": "BrokerageHistoryProvider",

  "algorithm-type-name": "Arbitrage",
  "algorithm-language": "Python",
  "algorithm-location": "../../../arbitrage/main.py",

  "data-folder": "../../../Data/",
  "cache-location": "../../../Data",
  "data-provider": "QuantConnect.Lean.Engine.DataFeeds.DefaultDataProvider",

  "log-handler": "QuantConnect.Logging.CompositeLogHandler",
  "messaging-handler": "QuantConnect.Messaging.Messaging",
  "job-queue-handler": "QuantConnect.Queues.JobQueue",
  "api-handler": "QuantConnect.Api.Api",
  "map-file-provider": "QuantConnect.Data.Auxiliary.LocalDiskMapFileProvider",
  "factor-file-provider": "QuantConnect.Data.Auxiliary.LocalDiskFactorFileProvider",

  "close-automatically": true,

  "maximum-warmup-history-days": 5,
  "maximum-chart-series": 10,

  "comment": "=== Multi-Account Configuration ===",
  "multi-account-config": {
    "IBKR": {
      "cash": 50000,
      "currency": "USD"
    },
    "Kraken": {
      "cash": 50000,
      "currency": "USD"
    }
  },

  "comment-routing": "=== Market-Based Order Routing ===",
  "routing-config": {
    "USA": "IBKR",
    "Kraken": "Kraken"
  },

  "comment-data": "=== Data Configuration ===",
  "data-channel-limit": 20000,

  "comment-kraken": "=== Kraken Configuration (模拟环境暂不需要真实API密钥) ===",
  "kraken-api-key": "",
  "kraken-api-secret": "",
  "kraken-verification-tier": "Intermediate",

  "comment-ibkr": "=== Interactive Brokers Configuration (模拟环境暂不需要真实账户) ===",
  "ib-account": "",
  "ib-user-name": "",
  "ib-trading-mode": "paper",
  "ib-enable-delayed-streaming-data": true,

  "comment-databento": "=== Databento Configuration (用于实时股票数据) ===",
  "databento-api-key": "",
  "databento-organization-id": "",

  "comment-strategy": "=== Strategy Parameters ===",
  "strategy-entry-threshold": -0.01,
  "strategy-exit-threshold": 0.02,
  "strategy-position-size-pct": 0.80,
  "strategy-aggression": 0.6,

  "comment-limits": "=== Trading Limits ===",
  "max-trading-pairs": 5,
  "max-position-per-pair": 100000
}
